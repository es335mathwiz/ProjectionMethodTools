package gov.frb.ma.msu.ProjectionTools;
import Jama.Matrix;
import junit.framework.TestCase;

public class SparseTest extends TestCase {
	double[][] A = {{1.,1.,1.},{1.,1.,1.},{1.,1.,1.}};
	double[][] B = {{2.,2.,2.},{2.,2.,2.},{2.,2.,2.}};
	ProjCodeMatrix matrix1 = new ProjCodeMatrix();
	ProjCodeMatrix matrix2 = new ProjCodeMatrix();
	
    public void testSparse(){
		matrix1.JamaRepresentation = new Matrix(A);
		matrix2.JamaRepresentation = new Matrix(B);

	MatrixStrategy Rule = new JamaMatrix();
	MatrixContext Billy = new MatrixContext(Rule);

	Billy.doAdd(matrix1,matrix2);
	String str = Billy.toString();
	System.out.println("This is the string: " + str);
    }
}
